{
  "name": "jQuery DOM",
  "version": "1",
  "manifest_version": 2,
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "matches": [ "http://*/*", "https://*/*"],
    "css": ["basic.css"],
    "js": [ "jquery.min.js", "teacup.js", "async.min.js", "background.js" ]
  }]
}
